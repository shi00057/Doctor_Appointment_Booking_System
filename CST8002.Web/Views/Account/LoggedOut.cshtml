@{
    ViewData["Title"] = "Logged Out";
}
<div class="container py-5 text-center">
    <h3 class="mb-3">Thank you for using the system</h3>
    <p class="text-muted">Redirecting to the login page...</p>
    <a class="btn btn-primary mt-3" asp-controller="Account" asp-action="Login">Go to Login</a>
</div>
<script>
    setTimeout(function () { window.location.href='@Url.Action("Login", "Account")'; }, 3000);
</script>
